</head>

<body>

<% //include ../partials/nav.ejs  %>
<form id="form">
  <textarea name="text">
  </textarea>
</form>
<button onclick="getGuesses()">Get guesses</button>

<div id="guesses"></div>
<script>
    function getGuesses() {
        $.get("/getGuessesDiv", $('#form').serialize(), function (data) {
            $('#guesses').html(data);
        })
    }
</script>
</body>
